{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "analyze": "turbo run analyze",
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "test:config": "turbo run test:config",
    "integration-test": "turbo run integration-test",
    "updateLPsAPR": "turbo run updateLPsAPR",
    "lint": "turbo run lint",
    "postinstall": "turbo run postinstall",
    "typechain": "turbo run typechain",
    "format:check:staged": "turbo run format:check:staged",
    "format:write": "turbo run format:write",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.10"
}
